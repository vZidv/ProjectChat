<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Background Colors-->
    <SolidColorBrush x:Key="BgColor1">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="BgColor2">#F5F5F7</SolidColorBrush>
    <SolidColorBrush x:Key="BgColor3">#EBECF0</SolidColorBrush>
    <SolidColorBrush x:Key="BgColor4">#E1E3E8</SolidColorBrush>

    <!--Main Colors-->
    <SolidColorBrush x:Key="Primary">#85D765</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryDark">#BFFAA8</SolidColorBrush>
    <SolidColorBrush x:Key="Second">#78909C</SolidColorBrush>
    <SolidColorBrush x:Key="Contrast">#263238</SolidColorBrush>
    
</ResourceDictionary>