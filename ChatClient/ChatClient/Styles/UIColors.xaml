<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Background Colors-->
    <SolidColorBrush x:Key="BgColor1">#0e1621</SolidColorBrush>
    <SolidColorBrush x:Key="BgColor2">#17212b</SolidColorBrush>
    <SolidColorBrush x:Key="BgColor3">#1b2734</SolidColorBrush>
    <SolidColorBrush x:Key="BgColor4">#242f3d</SolidColorBrush>
    
    <!--Main Colors-->
    <SolidColorBrush x:Key="Primary">#6ab3f3</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryDark">#2b5278</SolidColorBrush>
    <SolidColorBrush x:Key="Second">#768c9e</SolidColorBrush>
    <SolidColorBrush x:Key="ClearWhite">#f5f5f5</SolidColorBrush>

    <!--Color Palette-->
    
    <!-- Base Colors -->
    <SolidColorBrush x:Key="Red" Color="#FF0000"/>
    <SolidColorBrush x:Key="Green" Color="#00FF00"/>
    <SolidColorBrush x:Key="Blue" Color="#0000FF"/>
    <SolidColorBrush x:Key="Yellow" Color="#FFFF00"/>
    <SolidColorBrush x:Key="Purple" Color="#8000FF"/>
    <SolidColorBrush x:Key="Orange" Color="#FFA500"/>
    <SolidColorBrush x:Key="Pink" Color="#FF69B4"/>
    <SolidColorBrush x:Key="Cyan" Color="#00FFFF"/>
    <SolidColorBrush x:Key="Indigo" Color="#4B0082"/>
    <SolidColorBrush x:Key="Brown" Color="#8B4513"/>

    <!-- Dark Colors -->
    <SolidColorBrush x:Key="RedDark" Color="#CC0000"/>
    <SolidColorBrush x:Key="GreenDark" Color="#00CC00"/>
    <SolidColorBrush x:Key="BlueDark" Color="#0000CC"/>
    <SolidColorBrush x:Key="YellowDark" Color="#CCCC00"/>
    <SolidColorBrush x:Key="PurpleDark" Color="#6600CC"/>
    <SolidColorBrush x:Key="OrangeDark" Color="#CC8400"/>
    <SolidColorBrush x:Key="PinkDark" Color="#CC55A3"/>
    <SolidColorBrush x:Key="CyanDark" Color="#00CCCC"/>
    <SolidColorBrush x:Key="IndigoDark" Color="#3D0066"/>
    <SolidColorBrush x:Key="BrownDark" Color="#6B300D"/>

    <!-- Bright Colors -->
    <SolidColorBrush x:Key="RedLight" Color="#FF6666"/>
    <SolidColorBrush x:Key="GreenLight" Color="#66FF66"/>
    <SolidColorBrush x:Key="BlueLight" Color="#6666FF"/>
    <SolidColorBrush x:Key="YellowLight" Color="#FFFF66"/>
    <SolidColorBrush x:Key="PurpleLight" Color="#C0A0FF"/>
    <SolidColorBrush x:Key="OrangeLight" Color="#FFD700"/>
    <SolidColorBrush x:Key="PinkLight" Color="#FFA0C4E6"/>
    <SolidColorBrush x:Key="CyanLight" Color="#A0FFFF"/>
    <SolidColorBrush x:Key="IndigoLight" Color="#8A5FC2"/>
    <SolidColorBrush x:Key="BrownLight" Color="#A0654C"/>
    
</ResourceDictionary>